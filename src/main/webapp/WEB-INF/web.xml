    <exception-type>java.lang.Exception</exception-type>
    <location>/error.jsp</location>
  </error-page>
  
  
  <servlet-mapping>
    <servlet-name>CalculateShipping</servlet-name>
    <url-pattern>/CalculateShipping</url-pattern>
  </servlet-mapping>
